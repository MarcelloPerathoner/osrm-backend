{
  "name": "@project-osrm/osrm",
  "version": "6.0.0",
  "private": false,
  "type": "module",
  "description": "The Open Source Routing Machine is a high performance routing engine written in C++ designed to run on OpenStreetMap data.",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^2.0.0",
    "seedrandom": "^3.0.5"
  },
  "scripts": {
    "lint": "npx eslint features/",
    "test": "./scripts/cucumber_test_matrix.sh",
    "clean": "rm -rf test/cache",
    "docs": "make -C docs",
    "install": "node-pre-gyp install --fallback-to-build=false || ./scripts/node_install.sh",
    "nodejs-tests": "make -C test/data && build/nodejs/lib/binding_napi_v{napi_build_version}/osrm-datastore test/data/ch/monaco.osrm && node test/nodejs/index.js | faucet",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Project-OSRM/osrm-backend.git"
  },
  "author": "Project OSRM Team",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/Project-OSRM/osrm-backend/issues"
  },
  "homepage": "https://github.com/Project-OSRM/osrm-backend",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.5.0",
    "@mapbox/polyline": "^1.2.1",
    "@turf/turf": "^7.3.2",
    "acorn": "^8.15.0",
    "ansi-escape-sequences": "^6.2.4",
    "chalk": "^5.6.2",
    "cheap-ruler": "^4.0.0",
    "command-line-args": "^6.0.1",
    "command-line-usage": "^7.0.3",
    "csv-stringify": "^6.6.0",
    "d3-queue": "^3.0.7",
    "documentation": "^14.0.3",
    "eslint": "^9.39.2",
    "faucet": "^0.0.4",
    "flatbuffers": "^25.9.23",
    "github-markdown-css": "^5.9.0",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.11.1",
    "highlightjs-curl": "^1.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "marked": "^17.0.2",
    "marked-alert": "^2.1.2",
    "marked-custom-heading-id": "^2.0.16",
    "marked-footnote": "^1.4.0",
    "marked-gfm-heading-id": "^4.1.3",
    "marked-highlight": "^2.2.3",
    "marked-linkify-it": "^3.1.14",
    "mermaid": "^11.12.2",
    "node-addon-api": "^8.3.1",
    "node-cmake": "^2.5.1",
    "tape": "^5.9.0",
    "wait-on": "^9.0.3",
    "xmlbuilder": "15.1.1",
    "yargs": "^18.0.0"
  },
  "main": "lib/index.js",
  "binary": {
    "napi_versions": [
      8
    ],
    "module_name": "node_osrm",
    "module_path": "./lib/binding_napi_v{napi_build_version}/",
    "host": "https://github.com",
    "remote_path": "./Project-OSRM/osrm-backend/releases/download/v{version}/",
    "package_name": "{module_name}-v{version}-{napi_build_version}-{platform}-{arch}-{configuration}.tar.gz"
  },
  "lint-staged": {
    "features/**/*.js": [
      "node --check",
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
